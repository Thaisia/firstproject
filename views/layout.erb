<!DOCTYPE html>
<html>
  <head>
    <title>Taisiya Bo</title>
    <script type="text/javascript" src="/jquery.js"></script>
    <script src="/js/jquery-1.7.1.min.js" type="text/javascript"></script>
    <script src="/js/jquery.tilesgallery.js" type="text/javascript"></script>
    <script src="/myscripts.js" type="text/javascript"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/lightgallery/1.2.21/js/lg-fullscreen.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/lightgallery/1.2.21/js/lg-thumbnail.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/lightgallery/1.2.21/js/lightgallery-all.js"></script>
    <script src="https://use.fontawesome.com/886e6e746d.js"></script>
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="Stylesheet" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/lightbox2/2.8.2/css/lightbox.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Raleway:100,200,300,300i,500" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/lightgallery/1.2.21/css/lightgallery.css" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/lightgallery/1.2.21/css/lg-transitions.css" rel="stylesheet">
    <link href="/css/jquery-tilesgallery.css" rel="Stylesheet" />
    <link href="/mystyles.css" rel="stylesheet">
  </head>
<body>
    <div class="page">
      <div class="header">
        <div class="header-f">
          <span><h1><a href="/">Taisiya Bo</a></h1></span>
          <span class="h4"><a href="/">photoartist</a></span>
        </div>


        <span class="icons">
          <a href="/"><i class="fa fa-instagram" aria-hidden="true"></i></a>
          <a href="/"><i class="fa fa-vk" aria-hidden="true"></i></a>
          <a href="/"><i class="fa fa-facebook" aria-hidden="true"></i></a>
          <div class="contacts">
            <div>phone 322-223-66</div>
            <div>email ololo@dot.com</div>
          </div>
          <div class="useroptions">
            <a href="/"><span>ru eng latest</span></a>
            <a href="/"><span class="cow"><i class="fa fa-camera-retro" aria-hidden="true"></i></span></a>
            <a href="/"><span class="cob"><i class="fa fa-camera-retro" aria-hidden="true"></i></span></a>

          </div>
        </span>
      </div>

      <div class="sidebar sidebar-pic">
<!--       <div class="mini-menu">
          <ul>
            <li class="sub">

            </li>
            <li class="sub">
                <a href="/coaching">Coaching</a>
            </li>
            <li class="sub">
                <a href="/aboutme" >About me</a>
            </li>
          </ul>
        </div>-->



        <div class="mini-menu sidebar-themes">
          <ul>
            <% @themes.each do |theme| %>
              <% if theme['parent_id'] == 0 %>
                <li class="sub ajax">
                  <a href="/gallery/<%=theme['theme_link']%>"><%=theme['theme_name']%></a>
                    <ul>
                      <% @themes.each do |subtheme| %>
                        <% if subtheme['parent_id'] == theme['id'] %>
                          <li class="sub ajax">
                            <a href="/gallery/<%=subtheme['theme_link']%>"><%=subtheme['theme_name']%></a>
                              <ul>
                                <% @themes.each do |subsubtheme| %>
                                  <% if subsubtheme['parent_id'] == subtheme['id'] %>
                                    <li class="ajax">
                                      <a href="/gallery/<%=subsubtheme['theme_link']%>"><%=subsubtheme['theme_name']%></a>
                                    </li>
                                  <% end %>
                                <% end %>
                              </ul>
                          </li>
                        <% end %>
                      <% end %>
                    </ul>
                </li>
              <% end %>
            <% end %>
          </ul>
        </div>

        <ul class="sidebar-tags">
            <% @tags.each do |tag|%>
              <li><a href="/tags/<%= tag['tag_name']%>">#<%= tag['tag_name']%></a></li>
            <% end%>

        </ul>
      </div>
      <div class="gallery-container content">
            <%if @error && @error.length > 0 then %>
                <div><%=@error%></div>
            <% end %>
            <%= yield %>
        </div>
        <p style="clear:both;">
        </p>

        <script type="text/javascript">
        $(document).ready(function () {
          $('.sub > a').click(function(){
            clicked = $(this);
            $('.sub ul').each(function() {
              if (!($(this).is(clicked.closest("ul")))) {
                $(this).slideUp();
              }
            });

            if ($(this).next().is(":visible")){
              $(this).next().slideUp();
            } else {
              $(this).next().slideToggle();
            }
            return false;
          });

          $('.mini-menu > ul > li > a').click(function(){
            $('.mini-menu > ul > li > a, .sub a').removeClass('active');
            $(this).addClass('active');
          });

          $('.sub ul li a').click(function(){
            $('.sub ul li a').removeClass('active');
            $(this).addClass('active');
          });
        });
        </script>

        <script>
          function sendAjaxRequest(el) {
            $.ajax({
              url: el.attr("href") + "?ajax=1",
              success: function(res) {
                $("#ajaxresult").html(res);
                window.history.pushState(res, "", el.attr("href"));

                $(".tiles").tilesGallery({
                    height: 800,
                    margin: 4,
                    enableTwitter: true,
                    enableFacebook: true,
                    enableGplus: true,
                    enablePinterest: true
                });

                $("#lightgallery").lightGallery();

              }
            });

            return false;
          }

          $(document).ready(function() {
            $('.ajax > a').click(function(){
              return sendAjaxRequest($(this));
            });
          });
        </script>

        <script>
            $(function () {
                $(".tiles").tilesGallery({
                    height: 800,
                    margin: 4,
                    enableTwitter: true,
                    enableFacebook: true,
                    enableGplus: true,
                    enablePinterest: true
                });
            });
        </script>

        <script type="text/javascript">
            $(document).ready(function() {
                $("#lightgallery").lightGallery();
            });
        </script>

</body>
</html>
